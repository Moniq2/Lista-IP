#include<stdio.h>
int main(){
    int num, fator;
    scanf("%d", &num);
    printf("\n");

    for(int i = 0; i<num; i++){
        printf("\n");
        scanf("%d", &fator);
        printf("%d \t", fator);

        for (int j = fator; j > 0; j--)
        {
           printf("%d ", j);
    
        }
        
    }
    return 0;
}
